{
  "name": "@whimbrel/core",
  "version": "0.1.1",
  "description": "Whimbrel Core Library",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf ./dist && rimraf ./node_modules",
    "publish": "npm publish --access public"
  },
  "dependencies": {
    "@whimbrel/actor": "*",
    "@whimbrel/array": "*",
    "@whimbrel/core-api": "*",
    "@whimbrel/facet": "*",
    "@whimbrel/filesystem": "*",
    "@whimbrel/project": "*",
    "@whimbrel/source": "*",
    "@whimbrel/target": "*",
    "@whimbrel/walk": "*"
  },
  "devDependencies": {
    "@whimbrel-test/tree-fixtures": "*"
  }
}
