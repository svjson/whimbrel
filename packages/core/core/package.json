{
  "name": "@whimbrel/core",
  "version": "0.1.3",
  "description": "Whimbrel Core Library",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf ./dist && rimraf ./node_modules"
  },
  "dependencies": {
    "@whimbrel/actor": "workspace:*",
    "@whimbrel/array": "workspace:*",
    "@whimbrel/core-api": "workspace:*",
    "@whimbrel/facet": "workspace:*",
    "@whimbrel/filesystem": "workspace:*",
    "@whimbrel/project": "workspace:*",
    "@whimbrel/source": "workspace:*",
    "@whimbrel/target": "workspace:*",
    "@whimbrel/walk": "workspace:*",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@whimbrel-test/tree-fixtures": "workspace:*"
  }
}
