    "@whimbrel/source": "^0.0.1",
    "@whimbrel/target": "^0.0.1",
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "ora": "^8.2.0",
    "strip-ansi": "^7.1.0"
  },
  "dependencies": {
    "@whimbrel/actor": "*",
    "@whimbrel/core": "*",
    "@whimbrel/core-api": "*",
    "@whimbrel/gitignore": "*",
    "@whimbrel/package-json": "*",
    "@whimbrel/source": "*",
    "@whimbrel/target": "*",
    "chalk": "^5.6.0",
