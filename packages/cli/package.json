{
  "name": "whimbrel",
  "version": "0.0.1",
  "description": "CLI-based automation tool",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "whim": "./dist/index.cjs"
  },
  "exports": {
