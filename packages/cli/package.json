{
  "name": "whimbrel-cli",
  "version": "0.1.2",
  "description": "CLI-based automation and analysis tool",
  "license": "ISC",
  "author": "Sven Johansson",
  "homepage": "https://github.com/svjson/whimbrel",
  "repository": {
    "type": "git",
    "url": "https://github.com/svjson/whimbrel.git"
  },
  "bugs": {
    "url": "https://github.com/svjson/whimbrel/issues"
  },
  "engines": {
    "node": ">=20"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "whim": "dist/index.cjs"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf ./dist && rimraf ./node_modules"
  },
  "peerDependencies": {
    "@whimbrel/actor": "^0.1.2",
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "ora": "^8.2.0",
    "strip-ansi": "^7.1.0"
  },
  "dependencies": {
    "@whimbrel/core": "*",
    "@whimbrel/core-api": "*",
    "@whimbrel/docker-compose": "*",
    "@whimbrel/dotenv": "*",
    "@whimbrel/express": "*",
    "@whimbrel/fastify": "*",
    "@whimbrel/git": "*",
    "@whimbrel/gitignore": "*",
    "@whimbrel/koa": "*",
    "@whimbrel/license": "*",
    "@whimbrel/node": "*",
    "@whimbrel/npm": "*",
    "@whimbrel/package-json": "*",
    "@whimbrel/pnpm": "*",
    "@whimbrel/project": "*",
    "@whimbrel/source": "*",
    "@whimbrel/target": "*",
    "@whimbrel/tsconfig-json": "*",
    "@whimbrel/turborepo": "*",
    "@whimbrel/typescript": "*",
    "@whimbrel/vite": "*",
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "lodash.kebabcase": "^4.1.1",
    "ora": "^8.2.0",
    "strip-ansi": "^7.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@whimbrel/actor": "*",
    "rimraf": "^6.0.1",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
