{
  "name": "whimbrel",
  "version": "0.0.3",
  "description": "CLI-based automation tool",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "whim": "./dist/index.cjs"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf ./dist && rimraf ./node_modules",
    "publish": "npm publish --access public"
  },
  "peerDependencies": {
    "@whimbrel/actor": "^0.0.3",
    "@whimbrel/core": "^0.0.3",
    "@whimbrel/core-api": "^0.0.3",
    "@whimbrel/gitignore": "^0.0.3",
    "@whimbrel/node": "^0.0.3",
    "@whimbrel/npm": "^0.0.3",
    "@whimbrel/package-json": "^0.0.3",
    "@whimbrel/pnpm": "^0.0.3",
    "@whimbrel/project": "^0.0.3",
    "@whimbrel/source": "^0.0.3",
    "@whimbrel/target": "^0.0.3",
    "@whimbrel/tsconfig-json": "^0.0.3",
    "@whimbrel/turborepo": "^0.0.3",
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "ora": "^8.2.0",
    "strip-ansi": "^7.1.0"
  },
  "dependencies": {
    "chalk": "^5.6.0",
