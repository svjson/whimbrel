{
  "name": "@whimbrel/script-parser",
  "version": "0.1.1",
  "description": "Script parsing engine and IR for command-based script syntax variants",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf ./dist && rimraf ./node_modules"
  },
  "dependencies": {
    "@whimbrel/core-api": "*"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
