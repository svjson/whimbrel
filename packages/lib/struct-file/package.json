{
  "name": "@whimbrel/struct-file",
  "version": "0.1.3",
  "description": "Abstractions for common structured file formats",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf ./dist && rimraf ./node_modules"
  },
  "peerDependencies": {
    "@whimbrel/walk": "^0.1.3"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@whimbrel/filesystem": "workspace:*",
    "@whimbrel/walk": "workspace:*",
    "@whimbrel-test/context-fixtures": "workspace:*",
    "@whimbrel-test/tree-fixtures": "workspace:*",
    "vite": "^7.1.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
