{
  "name": "@whimbrel/flow",
  "version": "0.1.2",
  "description": "Fluent DSL for internal Whimbrel Action Execution",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf ./dist && rimraf ./node_modules"
  },
  "peerDependencies": {
    "@whimbrel/core-api": "^0.1.2"
  },
  "devDependencies": {
    "@whimbrel/core-api": "*",
    "@types/node": "^24.3.0",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
